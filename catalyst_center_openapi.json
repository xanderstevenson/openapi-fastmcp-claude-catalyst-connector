{
  "openapi": "3.0.1",
  "info": {
    "title": "Cisco DNA Center API",
    "version": "2.3.3.0"
  },
  "servers": [
    {
      "url": "/dna/intent/api/v1"
    }
  ],
  "paths": {
    "/network-device": {
      "get": {
        "tags": ["Devices"],
        "summary": "Get the list of network devices",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NetworkDeviceListResult"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "NetworkDeviceListResult": {
        "type": "object",
        "properties": {
          "response": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/NetworkDevice"
            }
          },
          "version": {
            "type": "string"
          }
        }
      },
      "NetworkDevice": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "hostname": {
            "type": "string"
          },
          "managementIpAddress": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "macAddress": {
            "type": "string"
          },
          "softwareVersion": {
            "type": "string"
          },
          "upTime": {
            "type": "string"
          },
          "role": {
            "type": "string"
          },
          "reachabilityStatus": {
            "type": "string"
          },
          "reachabilityFailureReason": {
            "type": "string"
          },
          "lastUpdated": {
            "type": "string",
            "format": "date-time"
          },
          "instanceUuid": {
            "type": "string"
          }
        }
      }
    }
  }
}
