{
  "mcpServers": {
    "catalyst-center": {
      "name": "Cisco Catalyst Center",
      "description": "Interact with Cisco Catalyst Center to manage network devices and configurations",
      "command": "python",
      "args": [
        "fastmcp_server_fixed.py"
      ],
      "env": {
        "CATALYST_BASE_URL": "${CATALYST_BASE_URL}",
        "CATALYST_USERNAME": "${CATALYST_USERNAME}",
        "CATALYST_PASSWORD": "${CATALYST_PASSWORD}",
        "PYTHONUNBUFFERED": "1"
      },
      "transport": {
        "type": "stdio",
        "timeout": 30
      },
      "singleInstance": true,
      "restartOnCrash": true,
      "startupTimeout": 10,
      "version": "1.0.0"
    }
  }
}
